{
  "name": "itsgoingtobe-react",
  "version": "0.0.1",
  "description": "It's Going To Be front-end with React, Redux, and React-Router!",
  "main": "index.js",
  "scripts": {
    "postinstall": "npm run parameters && npm run build",
    "clean": "rimraf web/client",
    "parameters": "cp -n app/Resources/client/project.config.js.dist app/Resources/client/project.config.js",
    "compile": "node app/Resources/client/build/scripts/compile",
    "build": "npm run clean && cross-env NODE_ENV=production npm run compile",
    "build-dev": "npm run clean && cross-env NODE_ENV=development npm run compile",
    "start": "cross-env NODE_ENV=server node app/Resources/client/build/scripts/start",
    "storybook": "start-storybook -p 6006 -c app/Resources/client/.storybook",
    "test": "cross-env NODE_ENV=test karma start app/Resources/client/build/karma.config",
    "test:watch": "npm test -- --watch",
    "lint": "eslint app/Resources/client/.",
    "lint:fix": "npm run lint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sproogen/itsgoingto.be"
  },
  "author": "James Grant <james@jwgmedia.co.uk> (http://jameswgrant.co.uk)",
  "license": "GPLv3",
