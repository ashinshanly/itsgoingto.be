{
  "name": "itsgoingtobe-react",
  "version": "0.0.1",
  "description": "It's Going To Be front-end with React, Redux, and React-Router!",
  "main": "main.js",
  "private": true,
  "scripts": {
    "postinstall": "npm run build",
    "start": "node scripts/start",
    "build": "cross-env PUBLIC_URL=/client node scripts/build",
    "test": "node scripts/test --env=jsdom",
    "lint": "eslint src/.",
    "storybook": "cross-env NODE_ENV=development start-storybook -p 6006 -c .storybook",
    "profile": "cross-env NODE_ENV=production BABEL_ENV=production GENERATE_SOURCEMAP=false webpack --config config/webpack.config.prod.js --profile --json > webpack-stats.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sproogen/itsgoingto.be"
  },
  "author": "James Grant <james@jwgmedia.co.uk> (http://jameswgrant.co.uk)",
  "license": "GPLv3",
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.7",
    "antd": "^2.13.6",
    "autoprefixer": "^8.4.1",
    "autosize": "^3.0.21",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-ramda": "^1.6.1",
    "babel-preset-react-app": "^3.1.1",
    "babel-runtime": "^6.26.0",
    "bourbon": "^4.3.4",
    "bourbon-neat": "^2.0.0",
    "cancelable-promise": "^2.4.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "^2.4.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-15": "^1.0.5",
    "eslint": "4.10.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "7.4.0",
    "extract-text-webpack-plugin": "3.0.2",
    "fbemitter": "^2.1.1",
    "file-loader": "^1.1.11",
    "font-awesome": "^4.7.0",
    "fs-extra": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "20.0.4",
    "moment": "^2.19.1",
    "node-sass": "^4.9.0",
    "object-assign": "^4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "^7.1.1",
    "raf": "3.4.0",
    "ramda": "^0.25.0",
    "react": "^15.6.2",
    "react-cookie": "^2.1.2",
    "react-countdown-now": "^1.2.0",
    "react-datepicker": "^0.53.0",
    "react-dev-utils": "^5.0.1",
    "react-dom": "^15.6.2",
    "react-helmet": "^5.2.0",
    "react-linkify": "^0.2.1",
    "react-rangeslider": "^2.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^3.2.0",
    "react-test-renderer": "^15.6.1",
    "redbox-react": "^1.5.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "resolve": "1.6.0",
    "sass-loader": "^6.0.5",
    "should": "^13.2.1",
    "should-enzyme": "^2.0.0",
    "style-loader": "^0.19.0",
    "url-loader": "^1.0.1",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "@storybook/react": "^3.4.3",
    "codacy-coverage": "^2.0.3"
  },
  "optionalDependencies": {
    "fsevents": "*"
  },
  "jest": {
    "coverageDirectory": "build/coverage",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/build/",
      "<rootDir>/scripts/",
      "<rootDir>/.storybook/",
      "<rootDir>/stories/",
      "<rootDir>/node_modules/"
    ],
    "coverageReporters": [
      "lcov"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/?(*.)(spec|test).{js,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js",
    "modulePaths": [
      "<rootDir>/src/"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
